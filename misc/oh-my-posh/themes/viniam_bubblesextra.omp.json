{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "right",
      "segments": [
        {
          "type": "shell",
          "style": "diamond",
          "background": "#29315A",
          "foreground": "#C792EA",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uf2bd {{ .UserName }}[{{ .Name }}]"
        },
        {
          "type": "git",
          "style": "diamond",
          "background": "#29315A",
          "foreground": "#5dabf3",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if( .UpstreamIcon ) }}{{ url .UpstreamIcon .UpstreamURL }}{{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#f64a69>\uf044 {{.Working.String}}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#49ef95>\uf046 {{.Staging.String}}</>{{ end }}{{ if gt .StashCount 0 }}  \ueb4b {{.StashCount}}{{ end }}{{ if gt .WorktreeCount 0 }}{{if .IsWorkTree}}<#1d953f>{{ end }}  \uf1bb {{if .IsWorkTree}}</>{{ end }}{{.WorktreeCount}}{{ end }}",
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#5dabf3{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#5dabf3{{ end }}",
            "{{ if gt .Ahead 0 }}#5dabf3{{ end }}",
            "{{ if gt .Behind 0 }}#5dabf3{{ end }}",
            "{{ if .UpstreamGone }}#5dabf3{{ end }}"
          ]
        },
        {
          "type": "python",
          "style": "diamond",
          "background": "#29315A",
          "foreground": "#E4F34A",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "gcp",
          "style": "diamond",
          "background": "#29315A",
          "foreground": "#5dabf3",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue7b2 {{.Project}}"
        },
        {
          "type": "upgrade",
          "style": "diamond",
          "foreground": "#111111",
          "background": "#49ef95",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4"
        },
        {
          "type": "time",
          "style": "diamond",
          "background": "#29315A",
          "foreground": "#AEA4BF",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uf017 {{ .CurrentDate | date .Format }}",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "executiontime",
          "style": "diamond",
          "background": "#29315A",
          "foreground": "#AEA4BF",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .FormattedMs }}",
          "properties": {
            "style": "austin",
            "threshold": 150
          }
        },
        {
          "type": "path",
          "style": "diamond",
          "background": "#29315A",
          "foreground": "#FF5370",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue5ff {{ path .Path .Location }} \u276f",
          "properties": {
            "style": "folder"
          }
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
